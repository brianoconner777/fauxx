    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$unauthenticated",
      "permission": "ALLOW",
      "property": "exists"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
